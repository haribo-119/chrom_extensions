{
    "manifest_version": 3,
    "name": "My Chrome App",
    "version": "1.0",
    "description": "A simple Chrome app.",
    "permissions": ["declarativeContent"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "TodoList.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    }
  }